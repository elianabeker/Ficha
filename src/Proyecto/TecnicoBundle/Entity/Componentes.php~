<?php

namespace Proyecto\TecnicoBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Componentes
 *
 * @ORM\Table()
 * @ORM\Entity
 */
class Componentes
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

  
     /**
     * @ORM\ManyToOne(targetEntity="Ficha", inversedBy="bienes")
     * @ORM\JoinColumn(name="ficha_id", referencedColumnName="id")
     */
    private $ficha;
    
     /**
     * @ORM\ManyToOne(targetEntity="TipoComponentes")
     * @ORM\JoinColumn(name="tipoComponente_id", referencedColumnName="id")
     */
    private $tipoComponentes;
    
    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

   
}